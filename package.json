{
	"name": "yester",
	"version": "0.6.1",
	"private": true,
	"scripts": {
		"start": "react-native start",
		"postversion": "react-native-version --never-amend",
		"start:ios": "react-native run-ios",
		"start:android": "react-native run-android",
		"test:lint": "eslint .",
		"pretest": "npm run test:lint",
		"test": "jest",
		"lint": "eslint ."
	},
	"dependencies": {
		"@react-native-community/viewpager": "^3.3.0",
		"@segment/analytics-react-native": "^1.1.0-beta.1",
		"amazon-cognito-identity-js": "^3.2.6",
		"aws-amplify": "^1.1.40",
		"aws-amplify-react-native": "^2.1.19",
		"axios": "^0.19.2",
		"base-64": "^0.1.0",
		"debug": "^4.1.1",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"prop-types": "^15.7.2",
		"react": "16.11.0",
		"react-native": "0.62.0",
		"react-native-datepicker": "^1.7.2",
		"react-native-device-info": "^3.1.4",
		"react-native-fbsdk": "^1.0.3",
		"react-native-gesture-handler": "^1.6.1",
		"react-native-keyboard-aware-scroll-view": "^0.9.1",
		"react-native-localization": "^2.1.6",
		"react-native-modal": "^11.5.6",
		"react-native-onesignal": "^3.6.5",
		"react-native-picker-select": "^6.6.0",
		"react-native-purchases": "^2.4.1",
		"react-native-reanimated": "^1.7.1",
		"react-native-splash-screen": "^3.2.0",
		"react-native-tab-view": "^2.13.0",
		"react-navigation": "^2.6.2",
		"suppplant": "^1.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/runtime": "^7.9.2",
		"@react-native-community/eslint-config": "^0.0.5",
		"babel-jest": "^25.2.3",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"eslint": "^6.8.0",
		"jest": "^25.2.3",
		"metro-react-native-babel-preset": "^0.59.0",
		"prettier": "^2.0.2",
		"react-dom": "^16.11.0",
		"react-native-dotenv": "^0.2.0",
		"react-native-version": "^3.1.0",
		"react-test-renderer": "^16.13.1"
	},
	"jest": {
		"preset": "react-native",
		"setupFiles": [
			"./jest-setup.js"
		],
		"coverageReporters": [
			"html"
		],
		"coverageThreshold": {
			"global": {
				"branches": 99,
				"functions": 99,
				"lines": 99,
				"statements": 99
			}
		},
		"collectCoverage": true,
		"transformIgnorePatterns": [
			"node_modules/(?!(jest-)?react-native|react-navigation)"
		],
		"collectCoverageFrom": [
			"src/**/*.js"
		],
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
		}
	}
}
